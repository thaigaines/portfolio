{
  "name": "portfolio",
  "version": "1.0.0",
  "homepage": "https://thaigaines.github.io",
  "scripts": {
    "build": "npx @tailwindcss/cli -i ./css/tailwind.css -o ./css/tailwind.min.css --minify",
    "watch": "npx @tailwindcss/cli -i ./css/tailwind.css -o ./css/tailwind.min.css --watch",
    "start": "http-server .",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d ."
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.13",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "http-server": "^14.1.1"
  }
}
